pragma solidity ^0.8.15;
contract practice{
    function even(uint256[] memory arrNum , uint256 arrLength) 
    public 
    pure 
    returns (uint256[] memory){
        for(uint256 i=0 ; i<arrLength ; i++){
            arrNum[i]=2*arrNum[i];
        }
        return arrNum;
    }
}
