pragma solidity ^0.8.15;
contract primeNumber{
    function prime(uint256 n) public pure returns (string memory){
        for(uint256 i =2 ; i<n ; i++){
            if(n % i ==0){
                return "Not a prime number";
            }
        }
        return "Prime";
    }
}

