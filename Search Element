pragma solidity ^0.8.15;
contract practice{
    function serach(uint256[] memory array,uint256 arrLength,uint256 elemToSearch)
    public pure returns(uint256){
        for(uint256 i=0 ; i < arrLength ; i++){
            if(array[i]==elemToSearch){
                return 1;
            }
        }
        return 0;
    }
}
